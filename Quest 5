public class Questao5 {
    public static void main(String[] args) {
        int[] vetor = new int[15];
        vetor[0] = 0;
        vetor[1] = 1;
        for (int i = 2; i < vetor.length; i++) {
            vetor[i] = vetor[i - 1] + vetor[i - 2];
        }
        for (int num : vetor) {
            System.out.print(num + " ");
        }
    }
}
