public class Questao4 {
    public static void main(String[] args) {
        int[] vetor = new int[10];
        for (int i = 0; i < vetor.length; i++) {
            vetor[i] = (i % 2 == 0) ? 1 : -1;
        }
        for (int num : vetor) {
            System.out.print(num + " ");
        }
    }
}
